
@model ProductViewModel
@{
    ViewData["Title"] = "Index";
}
<h4>Items In Category - @Model.BrandName</h4>
<ul class="list-group col-sm-8 col-xs-12">
    @{
        foreach (Products item in Model.Items)
        {
            <li class="list-group-item">
                @item.Description
            </li>
        }
    }
</ul>
